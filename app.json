{
  "expo": {
    "name": "DermaTrack",
    "slug": "DermaTrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AppIcons/Assets.xcassets/AppIcon.appiconset/1024.png",
    "scheme": "dermatrack",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/AppIcons/Assets.xcassets/AppIcon.appiconset/1024.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppIcons/playstore.png",
        "backgroundColor": "#000000"
      },
      "icon": "./assets/images/AppIcons/android/mipmap-xxxhdpi/ic_launcher.png",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/AppIcons/Assets.xcassets/AppIcon.appiconset/1024.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
